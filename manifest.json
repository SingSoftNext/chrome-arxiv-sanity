{
  "manifest_version": 2,

  "name": "arxiv-sanity",
  "description": "This extension loads the arxiv-sanity page for this arxiv page.",
  "version": "0.1",

  "browser_action": {
    "default_icon": "icon.png"
  },

  "background": {
      "scripts": ["background.js"],
      "persistent": false
  },

  "permissions": [
    "activeTab"
  ]
}
